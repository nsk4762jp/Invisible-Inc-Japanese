msgstr ""

#: STRINGS.AGENTS.XU.RESCUED
msgctxt "STRINGS.AGENTS.XU.RESCUED"
msgid "Excellent. Captivity does not agree with me."
msgstr ""

#: STRINGS.AGENTS.XU.YEARS_OF_SERVICE
msgctxt "STRINGS.AGENTS.XU.YEARS_OF_SERVICE"
msgid "3"
msgstr ""

